pipeline {
  agent any
  stages {
    stage('Install Jar') {
        steps {
            copyArtifacts filter: '**/TrainingApp-second.jar', projectName: 'build'
        }
    }
    stage('Run Jar') {
        steps {
            sh '''
            java -jar ${WORKSPACE}/eureka/target/TrainingApp-second.jar
            '''
        }
    }
  }
}