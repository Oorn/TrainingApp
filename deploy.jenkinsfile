pipeline {
  agent any
  stages {
    stage('Install Jar') {
        steps {
            copyArtifacts filter: '**/*.jar', projectName: 'build'
        }
    }
    stage('Run Jar') {
        steps {
            sh '''
            java -jar ${WORKSPACE}/eureka/target/TrainingApp-eureka.jar &\\
            java -jar ${WORKSPACE}/main-api/target/TrainingApp-main.jar &\\
            java -jar ${WORKSPACE}/second-api/target/TrainingApp-second.jar
            '''
        }
    }
  }
}