pipeline {
  agent any
  stages {
    stage('Install Jar') {
        steps {
            copyArtifacts filter: '**/*.jar', projectName: 'build'
        }
    }
    stage('Run Jar') {
        steps {
            sh "java -jar ${WORKSPACE}/target/TrainingApp-eureka.jar &\\
            java -jar ${WORKSPACE}/target/TrainingApp-main.jar &\\
            java -jar ${WORKSPACE}/target/TrainingApp-second.jar"
        }
    }
  }
}